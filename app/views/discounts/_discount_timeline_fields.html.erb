<div class="nested-fields">
  <div class="form-group">
    <%= f.label :start_date, "From", class: "col col-form-label" %>

    <div class="col-sm-10">
      <%= f.date_field :start_date, class: "form-control" %>
    </div>

    <%= f.label :end_date, "To", class: "col col-form-label" %>

    <div class="col-sm-10">
      <%= f.date_field :end_date, class: "form-control" %>
    </div>

    <div class="mt-3 ml-3">
      <%= link_to_remove_association "Delete", f, class: "btn-sm btn-danger text-white" %>
    </div>
  </div>
</div>
